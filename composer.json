{
    "name": "firehed/simplelogger",
    "description": "PHP write logs",
    "homepage": "https://github.com/firehed/simpleLogger",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Stern"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "psr/log": "^1.0"
    },
    "autoload": {
        "psr-4": {"Firehed\\SimpleLogger\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
          "Firehed\\SimpleLogger\\": "tests/"
        }
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpunit/phpunit": "^8.2 || ^9.0",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "scripts": {
        "test": [
          "@phpunit",
          "@phpstan",
          "@phpcs"
        ],
        "coverage": "phpunit --coverage-html coverage; open coverage/index.html",
        "autofix": "phpcbf",
        "phpunit": "phpunit",
        "phpstan": "phpstan analyse --no-progress",
        "phpcs": "phpcs"
    }
}
